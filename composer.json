{
  "name": "msbios/cpanel",
  "description": "MSBios CPanel",
  "license": "GPL",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-cpanel",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\CPanel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\CPanel\\": "tests/"
    }
  },
  "require": {
    "zendframework/zend-mvc-plugins": "^1.0",
    "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
    "msbios/view": "~1.0",
    "msbios/resource": "~1.0",
    "msbios/authentication": "~1.0",
    "msbios/guard": "~1.0",
    "msbios/guard-resource": "~1.0",
    "msbios/guard-cpanel": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "zendframework/zend-test": "^3.1",
    "squizlabs/php_codesniffer": "^3.0",
    "msbios/application": "~1.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit --colors=always"
    ],
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "assets": "cd bin && ln -s ../themes/limitless/public/ limitless",
    "serve": "php -S 0.0.0.0:3107 -t bin bin/msbios-cpanel-module.php",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  },
  "extra": {
    "zf": {
      "component": "MSBios\\CPanel"
    }
  }
}