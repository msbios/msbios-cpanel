{
  "name": "msbios/cpanel",
  "description": "MSBios CPanel",
  "license": "GPL",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-cpanel",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\CPanel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\CPanel\\": "tests/"
    }
  },
  "require": {
    "zendframework/zend-loader": "^2.5",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-modulemanager": "^2.8",
    "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
    "msbios/msbios": "^0.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "zendframework/zend-test": "^3.1",
    "squizlabs/php_codesniffer": "^3.0",
    "msbios/application": "dev-master",
    "msbios/authentication": "dev-master",
    "msbios/guard": "dev-master",
    "msbios/guard-cpanel": "dev-master",
    "msbios/guard-resource": "dev-master",
    "msbios/resource": "dev-master",
    "msbios/debug": "dev-master"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../msbios-application/"
    }, {
      "type": "path",
      "url": "../msbios-authentication/"
    }, {
      "type": "path",
      "url": "../msbios-guard/"
    }, {
      "type": "path",
      "url": "../msbios-guard-cpanel/"
    }, {
      "type": "path",
      "url": "../msbios-guard-resource/"
    }, {
      "type": "path",
      "url": "../msbios-resource/"
    }, {
      "type": "path",
      "url": "../msbios-debug/"
    }
  ],
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit"
    ],
    "assets": "cd bin && ln -s ../themes/limitless/public/ limitless",
    "serve": "php -S 0.0.0.0:3107 -t bin bin/msbios-cpanel-module.php",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  }
}
